			Type		MAC
			Comment		""
		End

		Begin COMBOBOX          LinkMode
                        Caption         "Link Mode "
                        Scale           0 0 0 0
                        ActiveOn        MODE_EDIT
                        Enabled         TRUE

                        Value           "Auto" "Manual"
                        Comment         "Link Mode"
                End

		Begin BUTTON		b_ok
			Caption		"OK"
			Scale		115 58 60 30
			ActiveOn	MODE_EDIT
			Action		ok
			Comment		"OK Button"
		End

		Begin BUTTON		b_cancel
			Caption		"Cancel"
			Scale		180 58 60 30
			ActiveOn	MODE_EDIT
			Action		cancel
			Comment		"Cancel Button"
		End

	EndInitVariableSection

	ExportSection

