		Caption		"Parameter Setting"
		FrameSize	250 100

		Begin TEXTLINE      macaddr
			Caption		"Mac Address "
			Scale		10 24 220 28
			ActiveOn	MODE_EDIT
			Enabled		FALSE
		    
			Type		MAC
			Comment		""
		End

		Begin COMBOBOX          LinkMode
                        Caption         "Link Mode "
                        Scale           0 0 0 0
                        ActiveOn        MODE_EDIT
                        Enabled         TRUE

                        Value           "Auto" "QPSK1/2" "QPSK3/4" "16QAM1/2" "16QAM3/4" "64QAM1/2" "64QAM2/3" "64QAM3/4"
                        Comment         "Link Mode"
                End

		Begin BUTTON		b_ok
			Caption		"OK"
			Scale		115 58 60 30
			ActiveOn	MODE_EDIT
			Action		ok
			Comment		"OK Button"
		End

		Begin BUTTON		b_cancel
			Caption		"Cancel"
			Scale		180 58 60 30
			ActiveOn	MODE_EDIT
			Action		cancel
			Comment		"Cancel Button"
		End

	EndInitVariableSection

	ExportSection

		Caption		""
		FrameSize       0 0
    
	EndExportSection
EndModuleSection

